{
  "name": "@sb-kit/ui",
  "version": "0.0.3",
  "author": "bytaesu",
  "description": "Shared UI package used across sb-kit.",
  "private": false,
  "exports": {
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.cjs"
    },
    "./styles/*": "./styles/*"
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "tsup",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@sb-kit/eslint-config": "^0.0.2",
    "@sb-kit/typescript-config": "^0.0.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.28.0",
    "tailwindcss": "^4",
    "tsup": "^8.5.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.476.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.57.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4",
    "zod": "^3.25.49"
  }
}
