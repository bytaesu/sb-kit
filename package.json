{
  "name": "sb-kit",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "build": "turbo run build",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,json}\"",
    "check-types": "turbo run check-types",
    "check": "pnpm run format && pnpm run lint && pnpm run check-types"
  },
  "devDependencies": {
    "turbo": "^2.5.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2"
  },
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": ">=18"
  }
}
